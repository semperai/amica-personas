{
  "name": "amica-multichain-indexer",
  "version": "1.0.0",
  "description": "Multichain Subsquid indexer for Amica personas",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "db:migrate": "npx squid-typeorm-migration apply",
    "processor:start": "node lib/main.js",
    "query-node:start": "squid-graphql-server",
    "api:start": "node lib/api-service.js",
    "dev": "npm run build && npm run processor:start",
    "codegen": "sqd codegen",
    "migration:generate": "sqd migration:generate",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@subsquid/evm-processor": "^1.16.1",
    "@subsquid/typeorm-store": "^1.4.0",
    "@subsquid/graphql-server": "^4.5.0",
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/evm-codec": "^1.2.2",
    "@subsquid/evm-abi": "^2.3.0",
    "ethers": "^6.9.0",
    "typeorm": "^0.3.17",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^3.3.2",
    "@subsquid/typeorm-codegen": "^1.3.3",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  }
}
