{
  "name": "amica",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:paths && npm run build:workers && vite",
    "build": "npm run generate:paths && npm run build:workers && vite build",
    "build:check": "npm run generate:paths && npm run build:workers && tsc && vite build",
    "build:workers": "npm run build:worker:generateMeshBVH",
    "build:worker:generateMeshBVH": "esbuild src/workers/bvh/generateMeshBVH.worker.js --bundle --minify --sourcemap --outfile=public/generateMeshBVH.worker.js",
    "preview": "vite preview",
    "generate:paths": "npx node scripts/generate_paths.js",
    "lint": "eslint src --ext ts,tsx",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=json --reporter=default --outputFile=test-results.json",
    "test:coverage": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "act": "act -j test"
  },
