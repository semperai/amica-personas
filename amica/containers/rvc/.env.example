# RVC (Retrieval-based Voice Conversion) Configuration

# Port to expose
RVC_PORT=7865

# Model name (must be placed in ./weights/ directory)
# Download models from: https://huggingface.co/models?search=rvc
RVC_MODEL_NAME=

# Index path (optional, for better quality)
RVC_INDEX_PATH=

# F0 up key (pitch adjustment, -12 to 12)
RVC_F0_UPKEY=0

# F0 method (harvest or pm)
# harvest: better quality but slower
# pm: faster but lower quality
RVC_F0_METHOD=harvest

# Index rate (0.0 to 1.0, default 0.66)
RVC_INDEX_RATE=0.66

# Filter radius (0 to 7, reduces breathiness)
RVC_FILTER_RADIUS=3

# Resample sample rate (0 to 48000, 0 = no resampling)
RVC_RESAMPLE_SR=0

# RMS mix rate (0.0 to 1.0, volume envelope)
RVC_RMS_MIX_RATE=0.25

# Protect (0.0 to 0.5, protects consonants)
RVC_PROTECT=0.33
